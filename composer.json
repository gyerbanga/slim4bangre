{
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "cakephp/chronos": "^1.2",
        "cakephp/database": "^3.8",
        "fig/http-message-util": "^1.1",
        "fullpipe/twig-webpack-extension": "^3.0",
        "monolog/monolog": "^2",
        "odan/phinx-migrations-generator": "^4.3",
        "odan/twig-translation": "^1.3",
        "php-di/php-di": "^6.0",
        "psr/log": "^1.1",
        "selective/array-reader": "^0.3",
        "selective/basepath": "^0.2",
        "selective/config": "^0.1.1",
        "selective/validation": "^0.7.1",
        "slim/http": "^1",
        "slim/psr7": "^1",
        "slim/slim": "^4.4",
        "slim/twig-view": "^3",
        "symfony/console": "^4.3 || ^5.0",
        "symfony/translation": "^4.3 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml",
        "test-coverage": "phpunit --configuration phpunit.xml --coverage-clover build/logs/clover.xml --coverage-html build/coverage"
    }
}
